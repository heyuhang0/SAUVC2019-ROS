<launch>
  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_localization_node">
    <param name="frequency" value="20" />
    <param name="sensor_timeout" value="0.1" />
    <param name="gravitational_acceleration" value="9.781" />

    <param name="odom_frame" value="odom"/>
    <param name="base_link_frame" value="bubbles/base_link"/>
    <param name="world_frame" value="odom"/>

    <param name="imu0" value="/bubbles/imu"/>
    <rosparam param="imu0_config">
      [false, false, false,
      true,  true,  true,
      false, false, false,
      false, false, false,
      true,  true,  true]
    </rosparam>
    <param name="imu0_remove_gravitational_acceleration" value="true"/>
    <param name="imu0_differential" value="false"/>
  </node>
</launch>